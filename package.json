{
  "name": "jidoyuro",
  "version": "0.0.1",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/local.js",
    "build": "tsc",
    "set-project": "gcloud config set project jidoyuro",
    "deploy": "tsc & gcloud functions deploy ynab --runtime nodejs12 --trigger-http --region=europe-west2 --timeout=540 --memory=1024MB"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.2.3",
    "@types/express": "^4.17.11",
    "playwright-aws-lambda": "^0.6.0",
    "playwright-core": "^1.8.0",
    "typescript": "^4.1.3",
    "ynab": "^1.21.0"
  },
  "devDependencies": {
    "tslint": "^6.1.3"
  }
}